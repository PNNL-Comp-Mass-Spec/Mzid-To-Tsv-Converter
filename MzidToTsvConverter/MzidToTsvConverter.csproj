<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFrameworks>net45;netcoreapp1.1</TargetFrameworks>
    <RootNamespace>MzidToTsvConverter</RootNamespace>
    <AssemblyName>MzidToTsvConverter</AssemblyName>
    <PackageId>MzidToTsvConverter</PackageId>
    <Version>1.1.0</Version>
    <AssemblyVersion>$(Version)</AssemblyVersion>
    <FileVersion>$(Version)</FileVersion>
    <Description>Converts mzid[.gz] files to the MS-GF+ tsv format. Designed for MS-GF+ mzid files (looks for EValue and SpecEValue scores)</Description>
    <Company>Pacific Northwest National Laboratory</Company>
    <Product>Mzid To Tsv Converter</Product>
    <Authors>PNNL-Comp-Mass-Spec</Authors>
    <RepositoryUrl>https://github.com/PNNL-Comp-Mass-Spec/Mzid-To-Tsv-Converter</RepositoryUrl>
    <RepositoryType>GitHub</RepositoryType>
    <Copyright>Copyright @ PNNL 2017</Copyright>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="PRISM-Library" Version="1.0.8" />
    <PackageReference Include="PSI_Interface" Version="1.2.0" />
  </ItemGroup>

</Project>